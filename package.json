{
  "name": "space-x-practice",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "api",
    "frontend"
  ],
  "scripts": {
    "build": "yarn workspace space-x-practice-api build && yarn workspace space-x-practice-frontend build",
    "start-backend": "yarn workspace space-x-practice-api start",
    "start-frontend": "yarn workspace space-x-practice-frontend start",
    "start": "npm-run-all --parallel start-backend start-frontend"
  },
  "engines": {
    "node": ">= 20.9.0",
    "npm": "please-use-yarn"
  },
  "dependencies": {
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.14",
    "npm-run-all": "^4.1.5"
  }
}
